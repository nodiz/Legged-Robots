function sln = run()
    q0 = [pi/10; -pi/10; 0];
    dq0 = [0;0;0];
   [ ~,~, ~, ~, num_steps] = control_hyper_parameters();
    
    
    x = optimize_k();
    Kp = x(1:2)
    Kd = x(3:4)

%     Kp = [300;200];
%     Kd = [Kp(1)/5;Kp(2)/12];
%     
    [sln, ~] = solve_eqns(q0,dq0,num_steps,Kp, Kd);
    animate(sln);

