control = 'PD'; %'PD' or 'VMC'
speed = 1; %1 to 9
[q0, dq0, ~, v_target, num_steps] = control_hyper_parameters()

switch control
    case 'PD'
       load('control_params_PD.mat', 'control_params');
       x = control_params;
       sln = solve_eqns(q0, dq0, , x(1:2), x(3:4),  x(5), x(6))
    case 'VMC'
       load('control_params_VMC.mat', 'control_params');
end

